# Unix
  alias      ll="ls -lah"
  alias      ..="cd .."
  alias     ...="cd ../.."
  alias    ....="cd ../../.."
  alias   .....="cd ../../../.."
  alias  ......="cd ../../../../.."
  alias      tc="time cat"
  alias      fc="vim ~/.config/fish/config.fish"
  alias      zc="vim ~/.zshrc"
  alias      vc="vim ~/.vimrc"
  alias      ac="vim ~/.aliases"
